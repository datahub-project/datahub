{
    "name": "hello-world-mfe",
    "version": "1.0.0",
    "description": "Hello World Micro-Frontend template for DataHub - Use this as a starting point for your custom MFE",
    "main": "src/index.tsx",
    "scripts": {
        "start": "webpack serve --mode development",
        "build": "webpack --mode production",
        "build:analyze": "webpack --mode production --analyze",
