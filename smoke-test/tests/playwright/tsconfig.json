{
  "compilerOptions": {
    "target": "ES2020",
    "lib": ["ES2020", "dom"],
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "types": ["node", "@playwright/test"]
  },
  "include": ["**/*.ts", "**/*.js"]
}
