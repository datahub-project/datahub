{
  "index": {
    "max_ngram_diff": 19,
    "analysis": {
      "filter": {
        "autocomplete_filter": {
          "type": "edge_ngram",
          "min_gram": "1",
          "max_gram": "20"
        },
        "custom_delimiter": {
          "split_on_numerics": "false",
          "split_on_case_change": "false",
          "type": "word_delimiter",
          "preserve_original": "true",
          "catenate_words": "false"
        }
      },
      "normalizer": {
        "custom_normalizer": {
          "filter": [
            "lowercase",
            "asciifolding"
          ],
          "type": "custom"
        }
      },
      "analyzer": {
        "delimit_edgengram": {
          "filter": [
            "lowercase",
            "custom_delimiter",
            "autocomplete_filter"
          ],
          "tokenizer": "whitespace"
        },
        "lowercase_keyword": {
          "filter": [
            "lowercase"
          ],
          "type": "custom",
          "tokenizer": "keyword"
        },
        "delimit": {
          "filter": [
            "lowercase",
            "custom_delimiter"
          ],
          "tokenizer": "whitespace"
        }
      },
      "tokenizer": {}
    }
  }
}
