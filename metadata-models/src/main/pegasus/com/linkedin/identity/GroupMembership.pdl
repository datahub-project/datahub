namespace com.linkedin.identity

import com.linkedin.common.Urn

/**
 * Carries information about the CorpGroups a user is in.
 */
@Aspect = {
  "name": "groupMembership"
}
record GroupMembership {
  @Relationship = {
    "/*": {
      "name": "IsMemberOfGroup",
      "entityTypes": [ "corpGroup" ]
    }
  }
  @Searchable = {
    "/*": {
       "fieldType": "URN",
       "addToFilters": true
    }
  }
  groups: array[Urn]
}
