namespace com.linkedin.role


/**
 * Provisioned users and groups of a role
 */
@Aspect = {
  "name": "actors"
}
record Actors {

  /**
   * List of provisioned users of a role
   */
  users: optional array[RoleUser]
  
  /**
   * List of provisioned groups of a role
   */
  groups: optional array[RoleGroup]

}
