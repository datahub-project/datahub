<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="metadata-io-es7-cassandra-neo4j" parallel="tests" thread-count="3">
    <test name="cassandra-testcontainers">
        <packages>
            <package name="com.linkedin.metadata.entity.cassandra.*" />
            <package name="com.linkedin.metadata.timeline.cassandra.*"/>
        </packages>
    </test>
    <test name="neo4j-testcontainers">
        <packages>
            <package name="com.linkedin.metadata.graph.neo4j.*" />
        </packages>
    </test>
    <test name="es7-testcontainers">
        <parameter name="elasticVersion" value="7.10.2"/>
        <packages>
            <package name="com.linkedin.metadata.graph.search.elasticsearch.*" />
            <package name="com.linkedin.metadata.search.elasticsearch.*"/>
        </packages>
    </test>
</suite>


