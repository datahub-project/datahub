{
  "type": "s3",
  "config": {
    "env": "UAT",
    "path_spec": {
      "include": "s3://my-test-bucket/folder_a/folder_aa/{dept}/{table}/*.*",
      "file_types": ["csv"],
      "table_name": "{dept}.{table}"
    },
    "aws_config": {
      "aws_region": "us-east-1",
      "aws_access_key_id": "testing",
      "aws_secret_access_key": "testing"
    },
    "profiling": {
      "enabled": false
    }
  }
}
